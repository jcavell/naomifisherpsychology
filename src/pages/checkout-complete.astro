---
export const prerender = false;

import BaseLayout from "../layouts/BaseLayout.astro";
import CheckoutCompleteComponent from "../components/checkout/CheckoutCompleteComponent";

const checkoutSessionId = Astro.cookies.get("checkout_session_id")?.value ?? null;
---
<BaseLayout active="checkout" pageTitle="Thank you">
    <CheckoutCompleteComponent client:load checkoutSessionId={checkoutSessionId}  />
</BaseLayout>