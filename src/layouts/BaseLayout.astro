---
import "../styles/v1/naomi.css";
// import "../styles/global.css";

import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";

const { pageTitle } = Astro.props;
const { active } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <meta name="robots" content="noindex, nofollow" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="description" content="" />
    <title>Naomi Fisher</title>
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,300..800;1,300..800&family=Oswald:wght@200..700&family=Yanone+Kaffeesatz:wght@200..700&display=swap"
      rel="stylesheet"
    />
  </head>

  <body class="category-primary-tagged category-courses">
    <Header active = {active}/>
    <slot />
    <Footer />
  </body>
</html>
